<%- model_class = Sku -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:sku_id) %>:</strong></dt>
  <dd><%= @sku.sku_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:num_iid) %>:</strong></dt>
  <dd><%= @sku.num_iid %></dd>
  <dt><strong><%= model_class.human_attribute_name(:properties) %>:</strong></dt>
  <dd><%= @sku.properties %></dd>
  <dt><strong><%= model_class.human_attribute_name(:properties_name) %>:</strong></dt>
  <dd><%= @sku.properties_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quantity) %>:</strong></dt>
  <dd><%= @sku.quantity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:product_id) %>:</strong></dt>
  <dd><%= @sku.product_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:account_id) %>:</strong></dt>
  <dd><%= @sku.account_id %></dd>
  <dt><strong></strong></dt>
  <dd></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              [@product,:skus], :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              [:edit, @product, @sku], :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              [@product, @sku],
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
