<p><h4>新增SKU</h4></p>
<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>商品ID</th>
      <th>SKU ID</th>
      <th>状态</th>
    </tr>
  </thead>
  <tbody>
    <% @sync_skus.latest.each_with_index do |sku,index| %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= sku.ware_id %></td>
      <td><%= sku.sku_id %></td>
      <td><%= sku.status_name %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<p><h4>属性变更SKU</h4></p>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>商品ID</th>
      <th>SKU ID</th>
    </tr>
  </thead>
  <tbody>
    <% @sync_skus.changed.each_with_index do |sku,index| %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= sku.ware_id %></td>
      <td><%= sku.sku_id %></td>
      <td><%= sku.status_name %></td>
      <td>
        <% sku.changes.each do |change| %>
        <strong><%= change.first %>:</strong><span class="label label-important">-<%= change.last[0] %></span>
        <span class="label label-success">+<%= change.last[1] %></span></br>
        <% end %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>