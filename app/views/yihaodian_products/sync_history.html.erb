  <li class="<%= params[:type] == "YihaodianSku"     ? 'active' : '' %>"><%= link_to "一号店商品SKU",sync_history_yihaodian_products_path(type: 'YihaodianSku') %></li>
</ul><!-- nav-tabs -->
<% end %>

<div class="container-fluid">
  <% if params[:type] == "YihaodianProduct" %>
    <%= render "products" %>
  <% elsif params[:type] == 'YihaodianSku' %>
    <%= render "skus" %>
  <% end %>
</div>