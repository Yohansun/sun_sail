<%= simple_form_for(@yihaodian_product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :product_code %>
    <%= f.input :product_cname %>
    <%= f.input :product_id %>
    <%= f.input :ean13 %>
    <%= f.input :category_id %>
    <%= f.input :can_sale %>
    <%= f.input :outer_id %>
    <%= f.input :can_show %>
    <%= f.input :verify_flg %>
    <%= f.input :is_dup_audit %>
    <%= f.input :prod_img %>
    <%= f.input :prod_detail_url %>
    <%= f.input :brand_id %>
    <%= f.input :merchant_category_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
