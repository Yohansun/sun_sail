<%#= render 'header' %>
<div class="row-fluid">
  
  <%= render 'select_time' %>

  <!-- <div class="controls clearfix">
    <div class="btns btn-group pull-left">
      <a href="#" class="btn btn-warning">导出数据</a>
    </div>
  </div> -->

  <div class="mar_t5">
    <ul class="nav nav-tabs reset_border nomargin" id="myTab_sub">
      <li><a href="/callcenter/contrastive_performance" id="success_rate_1">汇总</a></li>
      <li class="active"><a href="#" data-toggle="tab" id="success_rate_2">询单->下单</a></li>
      <!-- <li><a href="#tab3" data-toggle="tab" id="success_rate_3">下单->付款</a></li>
      <li><a href="#tab4" data-toggle="tab" id="success_rate_4">协助服务</a></li>
      <li><a href="#tab5" data-toggle="tab" id="success_rate_5">落实付款</a></li> -->
    </ul>

    <div class="tab-content reset_border">
      <div class="tab-pane active" id="tab2">
        <div class="re_margin30">
          <div class="control-group chart_opts">
            <div class="controls">
              <label class="radio inline">
                <input type="radio" checked="checked" value="daily_inquired_count_2" name="chart_option">
                询单人数
              </label>
              <label class="radio inline">
                <input type="radio" value="tomorrow_lost_count" name="chart_option">
                流失人数
              </label>
              <label class="radio inline">
                <input type="radio" value="daily_created_count" name="chart_option">
                当日下单人数
              </label>
              <label class="radio inline">
                <input type="radio" value="daily_created_payment" name="chart_option">
                当日下单金额
              </label>
              <label class="radio inline">
                <input type="radio" value="tomorrow_created_count" name="chart_option">
                最终下单人数
              </label>
              <label class="radio inline">
                <input type="radio" value="tomorrow_created_payment" name="chart_option">
                最终下单金额
              </label>
              <label class="radio inline">
                <input type="radio" value="success_ratio" name="chart_option">
                成功率
              </label>
            </div>
          </div>
            
          <div id="chartdiv" style="width: 100%; height: 400px;"></div>

          <table class="table table-striped table-bordered marginlr30 table-condensed td_colors mar_t">
            <thead>
              <tr>
                <th>昵称</th>
                <th>询单人数</th>
                <th>流失人数</th>
                <th>当日下单人数</th>
                <th>当日下单金额</th>
                <th>最终下单人数</th>
                <th>最终下单金额</th>
                <th>成功率</th>
              </tr>
            </thead>
            <tbody>
              <%- @members.each_with_index do |member, i|%>
                <tr id="member_<%= i+1 %>">
                  <td><%= member["_id"] %></td>
                  <td><%= member["value"]["daily_inquired_count"].try(:to_i) %></td>
                  <td><%= member["value"]["tomorrow_lost_count"].try(:to_i) %></td>
                  <td><%= member["value"]["daily_created_count"].try(:to_i) %></td>
                  <td><%= member["value"]["daily_created_payment"] %></td>
                  <td><%= member["value"]["tomorrow_created_count"].try(:to_i) %></td>
                  <td><%= member["value"]["tomorrow_created_payment"] %></td>
                  <%- if member["value"]["tomorrow_created_payment"] && member["value"]["daily_inquired_count"] && member["value"]["daily_inquired_count"] != 0 %>
                  <td><%= (member["value"]["tomorrow_created_count"].to_f/member["value"]["daily_inquired_count"]*100).round(2) %>%</td>
                  <%- else %>
                  <td>0%</td>
                  <%- end %>
                </tr>
              <%- end %>
              <%- @total_info.each do |info| %>
                <tr>
                  <td><%= info[0] %></td>
                  <td><%= info[1] %></td>
                  <td><%= info[2] %></td>
                  <td><%= info[3] %></td>
                  <td><%= info[4] %></td>
                  <td><%= info[5] %></td>
                  <td><%= info[6] %></td>
                  <td></td>
                </tr>
              <%- end %>
            </tbody>
          </table>
        </div>
      </div>      
    </div>
  </div>
</div>
<hr>
<div id="callcenter_tail">
  <%= render "tail" %>
<div>