<div class="controls">
	<label style="position: relative;">打印时间：</label>
	<input type="text" style="margin-right:5px;" placeholder="年-月-日" class="input-small print_start_date datepicker" value="<%= @from_deliver_print_date %>">
	<input type="text" placeholder="00:00:00" class="input-small print_start_time timepicker" value="<%= @from_deliver_print_time %>">
	<i class="icon-arrow-right"></i>
	<input type="text" style="margin-right:5px;" placeholder="年-月-日" class="input-small print_end_date datepicker" value="<%= @to_deliver_print_date %>">
	<input type="text" placeholder="00:00:00" class="input-small print_end_time timepicker" value="<%= @to_deliver_print_time %>">
 </div>